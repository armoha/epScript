function x() {
    // and/or operator precedence
    if(1 == 2 || 2 + 3 == 5 && 7 == 8) return 3;

    // Multiple or flattening
    if(Always() || Never() || Always()) return 5;

    // If ~ elseif
    if(1 == 2) return 3;
    else if(4 == 5) return 6;
    else {
        return 7;
    }

    // Not shortcut
    if(!Always()) return 1;
}
